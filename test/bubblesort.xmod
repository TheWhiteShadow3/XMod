n := input.size
while n > 1
{
    new_n := 1
    i := 0
    while (i < n - 1)
    {
        if (input[i] > input[i+1])
        {
            temp := input[i]
            input[i] := input[i+1]
            input[i+1] := temp
            new_n := i+1
        }
        i := i+1
    }
    n := new_n
}
